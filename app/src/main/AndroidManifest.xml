<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicplayer_hezhao">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity">
            <intent-filter>
                <action  android:name="android.intent.action.searchmusic"/>
                <category android:name="android.intent.category.searchmusic.search"/>
            </intent-filter>
        </activity>
        <activity android:name=".menu_work.change_background_activity"></activity>
        <activity android:name=".menu_work.nav_developer_msg_activity"></activity>
        <activity android:name=".menu_work.nav_listening_activity"></activity>
        <activity android:name=".menu_work.nav_msg_activity"></activity>
        <activity android:name=".menu_work.nav_order_activity"></activity>
        <activity android:name=".menu_work.nav_price_activity"></activity>
        <activity android:name=".menu_work.nav_show_activity"></activity>
        <activity android:name=".menu_work.nav_stop_activity"></activity>
        <activity android:name=".menu_work.nav_store_activity"></activity>
        <activity android:name=".LoginMainActivity"></activity>
        <activity android:name=".DownLoadMusic"></activity>
        <activity android:name=".HistoryMusic"></activity>
        <activity android:name=".LocalMusic"></activity>
    </application>

</manifest>